<div class="gallery span8">
  <% pet.get('followers').forEach(function(follower){ %>
    <div class="photo_container">
      <div class="photo_tag">
        <a href="#users/<%=follower.id%>"><%=follower.escape('username')%></a>
      </div>

      <div class="gallery_photo_div">
        <a href="#users/<%=follower.id%>">
          <img class="gallery_photo"
          src="<%=follower.escape('photo_url')%>" alt="<%=follower.escape('username')%>"
          height="100" width="100">
        </a>
      </div>
    </div>
  <% }); %>
</div>
<div class="back_button">
  <button class="btn back">Back</button>
</div>