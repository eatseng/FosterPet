<div class="row">
  <% pet.get('owners').forEach(function(owner){ %>
    <div class="col-xs-4">
      <div class="col-xs-offset-1">

        <a href="#users/<%=owner.id%>"><%=owner.get('username')%></a>

        <a href="#users/<%=owner.id%>">
          <img src="<%=owner.get('photo_url')%>" alt="<%=owner.get('username')%>"
          height="100" width="100">
        </a>
      </div>
    </div>
  <% }); %>

  <a href="/#pets/<%=pet.id%>">Back</a>
</div>

